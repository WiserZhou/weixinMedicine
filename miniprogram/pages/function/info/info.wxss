page {
  background: rgba(98, 248, 254,0.2);
  background: linear-gradient(90deg, rgba(98, 248, 254, 0.2) 0%, rgba(88, 214, 198, 0.2) 50%, rgba(0, 255, 141, 0.15) 97%);
}
.ins{
  margin-top: 20px;
  margin-left: 20px;
  padding: 10px;
  border-left: 7px solid rgb(43, 238, 245);
