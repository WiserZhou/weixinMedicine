}
.show {
  box-shadow: 0 2px 6px rgba(0, 0, 0, 0.2);
  transform: translateY(-5px);
  border-radius: 10px 10px 10px 10px;
  margin-left: 20px;
  margin-right: 20px;
  white-space: pre;
  padding: 10px;
